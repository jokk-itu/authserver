<form name="register" asp-action="connect/v1/account/register" method="post">
    <div class="form-group">
        <label class="form-label" for="givenname">Givenname</label>
        <input class="form-control form-control-lg" id="givenname" type="text" name="givenname"/>
    </div>
    <div class="form-group">
        <label class="form-label" for="familyname">Familyname</label>
        <input class="form-control form-control-lg" id="familyname" type="text" name="familyname"/>
    </div>
    <div class="form-group">
        <label class="form-label" for="middlename">Middlename</label>
        <input class="form-control form-control-lg" id="middlename" type="text" name="middlename"/>
    </div>
    <div class="form-group">
        <label class="form-label" for="nickname">Nickname</label>
        <input class="form-control form-control-lg" id="nickname" type="text" name="nickname"/>
    </div>
    <div class="form-group">
        <label class="form-label" for="address">Address</label>
        <input class="form-control form-control-lg" id="address" type="text" name="address"/>
    </div>
    <div class="form-group">
        <label class="form-label" for="locale">Locale</label>
        <input class="form-control form-control-lg" id="locale" type="text" name="locale"/>
    </div>
    <div class="form-group">
        <label class="form-label" for="gender">Gender</label>
        <select name="gender" form="register">
            <option value="male">Male</option>
            <option value="female">Female</option>
            <option value="none">Not specified</option>
        </select>
    </div>
    <div class="form-group">
        <label class="form-label" for="birthdate">Birthdate</label>
        <input class="form-control form-control-lg" id="birthdate" type="date" name="birthdate"/>
    </div>
    <div class="form-group">
        <label class="form-label" for="username">Username</label>
        <input class="form-control form-control-lg" id="username" type="text" name="username"/>
    </div>
    <div class="form-group">
        <label for="password" class="form-label">Password</label>
        <input class="form-control form-control-lg" id="password" type="password" name="Password"/>
    </div>
    <div class="form-group">
        <label class="form-label" for="email">Email</label>
        <input class="form-control form-control-lg" type="email" name="Email"/>
    </div>
    <div class="form-group">
        <label class="form-label" for="phonenumber">Phonenumber</label>
        <input class="form-control form-control-lg" type="tel" name="Phonenumber"/>
    </div>
    <input class="btn btn-primary" type="submit" value="Register" />
</form>